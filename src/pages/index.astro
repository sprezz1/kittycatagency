		<script>
			// Handle inquiry type change to show/hide appropriate fields
			const inquiryTypeSelect = document.getElementById('inquiryType');
			const salesFields = document.getElementById('salesFields');
			const generalFields = document.getElementById('generalFields');
			const messageField = document.getElementById('message');

			inquiryTypeSelect.addEventListener('change', function() {
				const selectedType = this.value;

				if (selectedType === 'sales') {
					salesFields.style.display = 'block';
					generalFields.style.display = 'none';
					messageField.removeAttribute('required');
				} else if (selectedType === 'general') {
					salesFields.style.display = 'none';
					generalFields.style.display = 'block';
					messageField.setAttribute('required', 'required');
				} else {
					salesFields.style.display = 'none';
					generalFields.style.display = 'none';
					messageField.removeAttribute('required');
				}
			});

			// Handle form submission
			const contactForm = document.getElementById('contactForm');
			const formMessage = document.getElementById('formMessage');
			const submitText = document.getElementById('submitText');
			const submitLoader = document.getElementById('submitLoader');

			contactForm.addEventListener('submit', async function(e) {
				e.preventDefault();

				// Show loading state
				submitText.style.display = 'none';
				submitLoader.style.display = 'inline';
				formMessage.className = 'form-message';
				formMessage.textContent = '';

				// Get form data
				const formData = new FormData(contactForm);
				const inquiryType = formData.get('inquiryType');

				// Build payload
				const payload = {
					domain: 'kittycatagency.com',
					inquiryType: inquiryType,
					email: formData.get('email'),
					timestamp: new Date().toISOString()
				};

				// Add fields based on inquiry type
				if (inquiryType === 'sales') {
					payload.companyName = formData.get('companyName') || '';
					payload.numModels = formData.get('numModels') || '';
					payload.telegram = formData.get('telegram') || '';
					payload.discord = formData.get('discord') || '';
					payload.country = formData.get('country') || '';
					payload.phone = formData.get('phone') || '';
				} else if (inquiryType === 'general') {
					payload.message = formData.get('message') || '';
				}

				try {
					// Send to n8n webhook
					const response = await fetch('https://n8ncontent.goshenducks.com/webhook/contact-form-submit', {
						method: 'POST',
						headers: {
							'Content-Type': 'application/json',
						},
						body: JSON.stringify(payload)
					});

					if (response.ok) {
						// Success
						formMessage.className = 'form-message success';
						formMessage.textContent = '‚úì Thanks! We\'ll get back to you within 24 hours.';
						contactForm.reset();
						salesFields.style.display = 'none';
						generalFields.style.display = 'none';
					} else {
						throw new Error('Server responded with an error');
					}
				} catch (error) {
					// Error
					console.error('Form submission error:', error);
					formMessage.className = 'form-message error';
					formMessage.textContent = '‚úó Something went wrong. Please try again or email us directly at hello@kittycatagency.com';
				} finally {
					// Reset button state
					submitText.style.display = 'inline';
					submitLoader.style.display = 'none';
				}
			});
		</script>
